// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model expense {
  index       Int      @id @default(autoincrement())
  id          String   @unique
  amount      String
  description String
  date        DateTime @default(now())

  @@index([id])
}
